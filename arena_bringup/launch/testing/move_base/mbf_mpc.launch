<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <!-- Arguments -->
  <arg name="model" />
  <arg name="speed" />

  <arg name="namespace" />
  <arg name="frame" />

  <include file="$(find arena_bringup)/launch/testing/move_base/mbf_nav/costmap_nav.launch">
    <arg name="model" value="$(arg model)" />
    <arg name="speed" value="$(arg speed)" />
    <arg name="namespace" value="$(arg namespace)" />
    <arg name="frame" value="$(arg frame)" />
  </include>

  <group ns="move_base_flex">
    <rosparam file="$(find arena_bringup)/params/mbf/mpc_local_planner_params.yaml"
      command="load" subst_value="True" />
    <rosparam
      file="$(find arena-simulation-setup)/robot/$(arg model)/configs/mbf/mpc_local_planner_params.yaml"
      command="load" subst_value="True" />
    <param name="base_local_planner" value="MpcLocalPlannerROS" />
  </group>

  <!-- temp -->
  <group ns="move_base_legacy_relay">
    <rosparam file="$(find arena_bringup)/params/mbf/mpc_local_planner_params.yaml"
      command="load" subst_value="True" />
    <rosparam
      file="$(find arena-simulation-setup)/robot/$(arg model)/configs/mbf/mpc_local_planner_params.yaml"
      command="load" subst_value="True" />
    <param name="base_local_planner" value="MpcLocalPlannerROS" />
  </group>
</launch>